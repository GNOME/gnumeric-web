all: manifest

manifest: Makefile
	echo $(WWWFILES) >$@

do-sync: ftp-sync manifest $(WWWFILES)
	./ftp-sync manifest
	git status --untracked-files=no

gnome-pages:
	mkdir public
	tar cf - $(WWWFILES) | tar xf - -C public
	(cd public && perl ../makerel $(WWWFILES))
